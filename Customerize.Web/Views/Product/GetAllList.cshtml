@model IEnumerable<ProductDtoList>
@{
    ViewData["Title"] = "GetAllList";
}

<h1>GetAllList</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Adı</th>
            <th scope="col">Kategori Adı</th>
            <th scope="col">Ürün Tipi</th>
            <th scope="col">Oluşturulma Zamanı</th>
            <th scope="col">Güncelleme Zamanı</th>
            <th scope="col">İşlemler</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.CategoryName</td>
                <td>@item.ProductTypeName</td>
                <td>@item.CreatedDate</td>
                <td>@item.UpdatedDate</td>
                <td>
                   <a class="btn btn-warning" asp-controller="" asp-action=""      asp-route-id="@item.Id">Güncelle</a>
                   <a class="btn btn-danger"  asp-controller="" asp-action=""      asp-route-id="@item.Id">Sil</a>
                </td>
            </tr>
        }
    </tbody>
</table>

