@model IEnumerable<CategoryDtoList>
@{
    ViewData["Title"] = "GetAllList";
}

<h1>GetAllList</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Adı</th>
            <th scope="col">Oluşturma Zamanı</th>
            <th scope="col">Güncelleme Zamanı</th>
            <th scope="col">İşlemler</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.CreatedDate</td>
                <td>@item.UpdatedDate</td>
                <td>
                   <a class="btn btn-warning" asp-controller="Category" asp-action="Edit"  asp-route-id="@item.Id">Güncelle</a>
                   <a class="btn btn-danger"  asp-controller="Category" asp-action=""      asp-route-id="@item.Id">Sil</a>
                   <a class="btn btn-success" asp-controller="Category" asp-action="Remove"      asp-route-id="@item.Id">Detay</a>
                </td>
            </tr>
        }
    </tbody>
</table>

