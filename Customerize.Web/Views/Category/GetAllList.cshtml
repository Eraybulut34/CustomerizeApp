@model IEnumerable<Customerize.Core.DTOs.Category.CategoryDtoList>
@{
    ViewData["Title"] = "GetAllList";
}

<h1>GetAllList</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Adı</th>
            <th scope="col">Oluşturma Zamanı</th>
            <th scope="col">Güncelleme Zamanı</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.CreatedDate</td>
                <td>@item.UpdatedDate</td>
            </tr>
        }
    </tbody>
</table>

